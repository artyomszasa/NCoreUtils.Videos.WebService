<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <AspNetCoreHostingModel>OutOfProcess</AspNetCoreHostingModel>
    <ServerGarbageCollection>false</ServerGarbageCollection>
    <RetainVMGarbageCollection>false</RetainVMGarbageCollection>
    <TargetLatestRuntimePatch>true</TargetLatestRuntimePatch>
    <PublishReadyToRun>true</PublishReadyToRun>
    <PublishReadyToRunShowWarnings>true</PublishReadyToRunShowWarnings>
    <PublishTrimmed>true</PublishTrimmed>
    <IsPackable>false</IsPackable>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\NCoreUtils.Videos.Shared\NCoreUtils.Videos.Shared.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Google.Cloud.Storage.V1" Version="3.1.0" />
    <PackageReference Include="NCoreUtils.AspNetCore.Logging.GoogleFluentd" Version="3.1.0-alpha1" />
    <PackageReference Include="NCoreUtils.AspNetCore.Proto" Version="3.1.0-alpha10" />
    <PackageReference Include="NCoreUtils.Images.GoogleCloudStorage" Version="3.1.0-alpha3" />
    <PackageReference Include="NCoreUtils.Images.WebService.Client" Version="3.1.0-alpha4" />
    <PackageReference Include="NCoreUtils.IO" Version="3.1.0-alpha0" />
    <PackageReference Include="Xabe.FFMpeg" Version="4.1.0" />
  </ItemGroup>
</Project>
